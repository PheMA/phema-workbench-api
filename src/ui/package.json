{
  "name": "phex",
  "version": "1.0.0",
  "description": "PhEx User Interface",
  "main": "dist/index.js",
  "scripts": {
    "start": "yarn run build-monaco-workers && parcel --port 8989 serve -d ../api/main/resources/ui index.html",
    "watch": "parcel --port 8989 watch -d ../../target/classes/ui index.html",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "yarn run build-monaco-workers && parcel build -d ../api/main/resources/ui index.html",
    "build-monaco-workers": "yarn run build-worker",
    "build-json": "parcel build node_modules/monaco-editor/esm/vs/language/json/json.worker.js --no-source-maps",
    "build-css": "parcel build node_modules/monaco-editor/esm/vs/language/css/css.worker.js --no-source-maps",
    "build-html": "parcel build node_modules/monaco-editor/esm/vs/language/html/html.worker.js --no-source-maps",
    "build-ts": "parcel build node_modules/monaco-editor/esm/vs/language/typescript/ts.worker.js --no-source-maps",
    "build-worker": "parcel build -d ../api/main/resources/ui node_modules/monaco-editor/esm/vs/editor/editor.worker.js --no-source-maps",
    "testing": "parcel"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7",
    "@babel/polyfill": "^7",
    "@blueprintjs/core": "^3.10.0",
    "@blueprintjs/icons": "^3.4.0",
    "@blueprintjs/select": "^3.11.1",
    "es6-shim": "^0.35.4",
    "jsoneditor": "^7.1.0",
    "jsoneditor-react": "^2.0.0",
    "localforage": "^1.7.3",
    "lodash": "^4.17.13",
    "moment": "^2.23.0",
    "monaco-editor": "^0.18.1",
    "numeral": "^2.0.6",
    "prism-react-renderer": "^1.0.2",
    "prop-types": "^15.6.2",
    "react": "^16.2.0",
    "react-dom": "^15.3.0 || 16",
    "react-dropzone": "^v10.1.4",
    "react-json-view": "^1.19.1",
    "react-redux": "^6.0.0",
    "react-split-pane": "^0.1.84",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.0",
    "uui": "^1.0.7"
  },
  "devDependencies": {
    "parcel": "^1.12.4",
    "babel-preset-env": "^1.7.0",
    "eslint": "^6.8.0",
    "sass": "^1.15.1"
  }
}
